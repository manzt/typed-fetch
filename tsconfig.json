{
  "include": ["src/*"],
  "compilerOptions": {
    "module": "node16",
    "moduleResolution": "node16",
    "moduleDetection": "force",
    "target": "ES2020",
    "lib": ["DOM", "DOM.Iterable", "ES2020"],
    "declaration": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true
  }
}
